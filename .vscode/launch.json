{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_DEBUG": "0"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload",
        "--port",
        "8000",
        "--host",
        "localhost"
      ],
      "jinja": true
    }
  ]
}
